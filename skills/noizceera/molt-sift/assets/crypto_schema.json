{
  "type": "object",
  "title": "Cryptocurrency Price Data",
  "description": "Schema for validating crypto market data",
  "required": ["symbol", "price"],
  "properties": {
    "symbol": {
      "type": "string",
      "description": "Cryptocurrency ticker symbol (e.g., BTC, ETH, SOL)"
    },
    "price": {
      "type": "number",
      "description": "Current price in USD",
      "minimum": 0
    },
    "volume": {
      "type": "number",
      "description": "24h trading volume in USD",
      "minimum": 0
    },
    "change_pct": {
      "type": "number",
      "description": "24h price change percentage",
      "minimum": -100
    },
    "high_24h": {
      "type": "number",
      "description": "24h high price",
      "minimum": 0
    },
    "low_24h": {
      "type": "number",
      "description": "24h low price",
      "minimum": 0
    },
    "timestamp": {
      "type": "string",
      "description": "ISO 8601 timestamp"
    },
    "source": {
      "type": "string",
      "description": "Data source (binance, coinbase, kraken, etc)"
    }
  }
}
