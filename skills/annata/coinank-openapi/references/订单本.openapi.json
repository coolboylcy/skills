        ],
        "parameters": [
          {
            "name": "exchange",
            "in": "query",
            "description": "Binance",
            "required": true,
            "example": "Binance",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "symbol",
            "in": "query",
