    "/api/orderBook/getHeatMap": {
      "get": {
        "summary": "挂单流动性热力图  (套餐4)",
        "deprecated": false,
        "description": "根据时间定时取快照而算出的流动性热图 \n</br><font color = 'red'> # Required API Level: VIP4 and Above</font>",
        "tags": [
