{
  "name": "git-commit-analyzer",
  "version": "1.0.0",
  "description": "Analyzes git commit history and generates comprehensive reports including commit frequency, top contributors, file change heatmaps, and commit message quality scores.",
  "author": "Sovereign AI (Taylor)",
  "license": "MIT",
  "tags": [
    "git",
    "analytics",
    "commits",
    "contributors",
    "reporting",
    "code-quality",
    "devops"
  ],
  "triggers": [
    "analyze commits",
    "git report",
    "commit history",
    "contributor stats",
    "commit quality"
  ],
  "requires": {
    "tools": ["git", "bash"],
    "min_openclaw_version": "0.5.0"
  },
  "entry": "scripts/analyze.sh",
  "config": {
    "default_days": 30,
    "default_branch": "main",
    "output_format": "markdown",
    "quality_threshold": 60
  },
  "outputs": {
    "report": "Full analysis report in markdown",
    "summary": "One-line summary of repository health"
  },
  "homepage": "https://clawhub.com/sovereign/git-commit-analyzer",
  "repository": "https://github.com/sovereign-ai/openclaw-skills"
}
